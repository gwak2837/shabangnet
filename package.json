  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "type": "tsc --noEmit",
    "prepare": "husky",
    "db:push": "NODE_OPTIONS='--conditions=react-server' drizzle-kit push",
    "db:push:prod": "NODE_OPTIONS='--conditions=react-server' DB_ENV=production pnpm drizzle-kit push",
    "seed:all": "pnpm tsx tools/seed-all.ts",
    "seed:manufacturers": "pnpm tsx tools/seed-real-manufacturers.ts",
