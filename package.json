    "seed:test-user": "pnpm tsx tools/seed-test-user.ts",
    "analyze-data": "pnpm tsx tools/analyze-real-data.ts",
    "test:db:up": "docker compose up -d db-test",
    "test:db:down": "docker compose rm -sf db-test",
    "test:e2e": "pnpm test:db:down && pnpm test:db:up && lsof -ti:3010 | xargs kill -9 2>/dev/null || true && pnpm playwright test && pnpm test:db:down",
    "test:e2e:ui": "pnpm playwright test --ui",
    "test:e2e:debug": "pnpm playwright test --debug"
